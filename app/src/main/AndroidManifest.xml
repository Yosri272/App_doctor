<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.app_doctor">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ahjez"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity       android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login_doctor"
                  android:label="تسجيل الدخول"/>
        <activity android:name=".create_doctor"
                  android:label="انشاء حساب"/>
        <activity android:name=".hell_doctor" />
        <activity android:name=".modf"
                  android:label="تعديل في ايام العمل"/>
        <activity android:name=".find" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".phone"
                  android:label="ارسال رقم الهاتف"/>
    </application>

</manifest>