<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@drawable/halaby">
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/scrollView"

        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/d1" android:inputType="number"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="الرقم التعريفي "/>
            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/d2"
                android:hint="الاسم المستخدم"
                android:layout_below="@+id/d1"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/d3"
                android:hint="كلمة السر"
                android:layout_below="@+id/d2"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="اعادة كلمة السر"
                android:layout_below="@+id/d3"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:id="@+id/d9" />



            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:id="@+id/p7"
                android:hint="قيمة المقابلة"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />


            <TextView
                android:id="@+id/textView9"
                android:layout_width="122dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"

                android:layout_below="@+id/p6"
                android:layout_marginLeft="88dp"
                android:layout_marginStart="88dp"
                android:layout_marginTop="35dp"
                android:text="زمن المقابلة"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="122dp"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/p6"
                android:layout_alignRight="@+id/p6"

                android:layout_alignTop="@+id/textView9"
                android:text="ايام المقابلة"
                android:textAppearance="?android:attr/textAppearanceMedium" />



            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText
                    android:id="@+id/y1"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/m2"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:ems="10"
                    android:hint="الي"
                    android:inputType="time" />

                <EditText
                    android:id="@+id/m1"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/m1"
                    android:layout_below="@+id/textView9"
                    android:layout_centerHorizontal="true"
                    android:ems="10"
                    android:hint="من"
                    android:inputType="time" />

                <CheckBox
                    android:id="@+id/sat"
                    android:layout_width="83dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/m1"
                    android:layout_alignEnd="@+id/textView7"
                    android:layout_alignRight="@+id/textView7"
                    android:layout_column="3"
                    android:layout_row="0"
                    android:checked="false"

                    android:text="السبت" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText
                    android:id="@+id/y2"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/m2"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:ems="10"
                    android:hint="الي"
                    android:inputType="time" />

                <EditText
                    android:id="@+id/m2"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:ems="10"
                    android:hint="من"
                    android:inputType="time" />

                <CheckBox
                    android:id="@+id/sat1"
                    android:layout_width="83dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/m1"
                    android:layout_alignEnd="@+id/textView7"
                    android:layout_alignRight="@+id/textView7"
                    android:layout_column="3"
                    android:layout_row="0"
                    android:checked="false"
                    android:text="الاحد" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText
                    android:id="@+id/y3"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/y3"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:ems="10"
                    android:hint="الي"
                    android:inputType="time" />

                <EditText
                    android:id="@+id/m3"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView9"
                    android:layout_centerHorizontal="true"
                    android:ems="10"
                    android:hint="من"
                    android:inputType="time" />

                <CheckBox
                    android:id="@+id/sat2"
                    android:layout_width="83dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/y4"
                    android:layout_alignEnd="@+id/textView7"
                    android:layout_alignRight="@+id/textView7"
                    android:layout_column="3"
                    android:layout_row="0"
                    android:checked="false"
                    android:text="الاثنين" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText
                    android:id="@+id/y4"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/y4"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:ems="10"
                    android:hint="الي"
                    android:inputType="time" />

                <EditText
                    android:id="@+id/m4"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView9"
                    android:layout_centerHorizontal="true"
                    android:ems="10"
                    android:hint="من"
                    android:inputType="time" />

                <CheckBox
                    android:id="@+id/sat3"
                    android:layout_width="83dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/m1"
                    android:layout_alignEnd="@+id/textView7"
                    android:layout_alignRight="@+id/textView7"
                    android:layout_column="3"
                    android:layout_row="0"
                    android:checked="false"
                    android:text="الثلاثاء" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText
                    android:id="@+id/y5"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/y5"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:ems="10"
                    android:hint="الي"
                    android:inputType="time" />

                <EditText
                    android:id="@+id/m5"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView9"
                    android:layout_centerHorizontal="true"
                    android:ems="10"
                    android:hint="من"
                    android:inputType="time" />

                <CheckBox
                    android:id="@+id/sat4"
                    android:layout_width="83dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/m1"
                    android:layout_alignEnd="@+id/textView7"
                    android:layout_alignRight="@+id/textView7"
                    android:layout_column="3"
                    android:layout_row="0"
                    android:checked="false"

                    android:text="الاربعاء" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <EditText
                    android:id="@+id/y6"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/y7"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:ems="10"
                    android:hint="الي"
                    android:inputType="time" />

                <EditText
                    android:id="@+id/m6"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView9"
                    android:layout_centerHorizontal="true"
                    android:ems="10"
                    android:hint="من"
                    android:inputType="time" />

                <CheckBox
                    android:id="@+id/sat5"
                    android:layout_width="83dp"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/m1"
                    android:layout_alignEnd="@+id/textView7"
                    android:layout_alignRight="@+id/textView7"
                    android:layout_column="3"
                    android:layout_row="0"
                    android:checked="false"

                    android:text="الخميس" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <Button
                    android:id="@+id/d4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rio"
                    android:onClick="but20"
                    android:text="انشاء حساب طبيب"
                    android:layout_below="@+id/gridLayout3"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="88dp" />
            </TableRow>
        </TableLayout>
    </ScrollView>

</RelativeLayout>
